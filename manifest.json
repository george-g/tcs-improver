{
  "name": "TCSI",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension improve TCS usability.",
  "update_url": "http://tcs-improver.googlecode.com/files/googlechrome_updates.xml",
  "browser_action": {
    "default_icon": "icon.png"
  },  
  "content_scripts": [
    {
      "matches": ["*://*.gehtsoft.com:*/tcs/*"],
      "js": ["jquery-1.7.2.min.js","tcsi.js"],
      "run_at": "document_end"
    }
  ]
}
